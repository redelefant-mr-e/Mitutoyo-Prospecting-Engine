{
  "name": "clay-csv-viewer",
  "version": "1.0.0",
  "description": "A modern CSV viewer for Clay data with JSON support and intelligent data analysis",
  "main": "electron.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-build": "npm run build && electron .",
    "package": "npm run build && electron-builder",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "keywords": [
    "csv",
    "viewer",
    "data-analysis",
    "json",
    "react",
    "vite",
    "clay",
    "data-visualization",
    "table-viewer",
    "file-upload"
  ],
  "author": "redelefant-mr-e",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/redelefant-mr-e/Mitutoyo-Prospecting-Engine.git"
  },
  "bugs": {
    "url": "https://github.com/redelefant-mr-e/Mitutoyo-Prospecting-Engine/issues"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "lucide-react": "^0.294.0",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.6.4",
    "gh-pages": "^6.3.0",
    "vite": "^5.0.8",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.redelefant-mr-e.mitutoyo-prospecting-engine",
    "productName": "Mitutoyo Prospecting Engine",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron.js",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
